    "nodemon": "1.11.0",
    "webpack": "2.2.1"
  },
  "dependencies": {
    "body-parser": "1.17.1",
    "express": "4.15.2",
    "mongoose": "4.9.0",
    "validator": "7.0.0"
  }
}
