{
  "name": "setup",
  "version": "1.0.0",
  "description": "initial setup for a node web app with webpack, babel, eslint, less",
  "main": "server.js",
  "module": "module.js",
  "scripts": {
    "dev": "rm -rf ./client/dist/*.* && webpack-dev-server --env development --progress",
    "build": "webpack --env production",
    "build:styles": "lessc ./client/src/styles/main.less ./client/dist/main.css",
    "lint:js": "eslint ./ webpack.*.js --fix --cache",
    "server": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "webpack",
    "babel",
    "node",
    "and",
    "more"
  ],
  "author": "Miguel Julio <miguel.julio.developer@gmail.com> (http://fitfab.com/)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-loader": "6.4.1",
    "babel-preset-env": "1.2.2",
    "css-loader": "0.27.3",
    "eslint": "3.18.0",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "2.1.0",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "moment": "2.17.1",
    "postcss-loader": "1.3.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.17.1",
    "express": "4.15.2",
    "mongoose": "4.9.0",
    "validator": "7.0.0"
  }
}
